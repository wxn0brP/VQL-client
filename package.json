{
    "name": "@wxn0brp/vql-client",
    "version": "0.2.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "description": "VQL Client",
    "homepage": "https://github.com/wxn0brP/VQL-client",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/VQL-client.git"
    },
